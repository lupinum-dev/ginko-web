{
  "name": "ginko-web",
  "version": "0.0.5",
  "description": "Ginko Web is a plugin that allows you to publish your Obsidian notes to the web.",
  "author": "Lupinum OG",
  "license": "BSL-1.1",
  "keywords": [
    "ginko",
    "web",
    "obsidian",
    "markdown",
    "publish",
    "website",
    "blog",
    "documentation",
    "knowledge base",
    "wiki"
  ],
  "main": "main.ts",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "lint": "eslint .",
    "lintfix": "eslint . --fix"
  },
  "dependencies": {
    "@codemirror/language": "^6.10.8",
    "@codemirror/rangeset": "^0.19.9",
    "@codemirror/state": "^6.5.0",
    "@codemirror/view": "^6.36.1",
    "confbox": "^0.1.8",
    "image-meta": "^0.2.1",
    "minimatch": "^9.0.0",
    "nanoid": "^5.0.9",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "unctx": "^2.4.1",
    "uuid": "^11.0.4",
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.1.0",
    "@types/codemirror": "^5.60.15",
    "@types/node": "^16.11.6",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@vue/compiler-sfc": "^3.3.0",
    "builtin-modules": "3.3.0",
    "dotenv": "^16.4.7",
    "esbuild": "0.17.3",
    "esbuild-plugin-vue3": "^0.4.2",
    "eslint": "^9.19.0",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "5.7.2"
  }
}